(require racket/trace)
(define (make-interval a b)
  (cons a b)
  )
(define (upper-bound t)
  (max (car t) (cdr t))
  )
(define (lower-bound t)
  (min (car t) (cdr t))
  )
(define t (make-interval 3 4))
(upper-bound t)
(lower-bound t)
(define x (make-interval 4 3))
(upper-bound x) 
(lower-bound x)

