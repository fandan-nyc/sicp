(require racket/trace)
(define (make-interval a b)
  (cons a b)
  )
(define (upper-bound t)
  (max (car t) (cdr t))
  )
(define (lower-bound t)
  (min (car t) (cdr t))
  )
(define t (make-interval 3 4))
(upper-bound t)
(lower-bound t)
(define x (make-interval 4 3))
(upper-bound x) 
(lower-bound x)

(define (sub-interval a b)
  (make-interval 
   ( - (lower-bound a) ( upper-bound b))
   ( - (upper-bound a) ( lower-bound b))
   )
  )
(sub-interval t x)
(sub-interval x t)

 ;; Usage 
(define i (make-interval 2 7)) 
(define j (make-interval 8 3)) 
  
(sub-interval i j)
(sub-interval j i)
